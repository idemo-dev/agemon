{
  "name": "agemon",
  "version": "0.1.0",
  "description": "Visualize, Grow, and Evolve your AI Dev Environment - AgentMonster",
  "type": "module",
  "bin": {
    "agemon": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "npm run build:cli && npm run build:ui",
    "build:cli": "tsup",
    "build:ui": "vite build",
    "dev:ui": "vite",
    "dev": "tsx src/cli/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "developer-tools",
    "cli",
    "claude-code",
    "codex",
    "gamification",
    "visualization"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "commander": "^14.0.3",
    "express": "^5.2.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
